{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/dist/Home.html"
    },
    {
      "src": "^/docs/?$",
      "dest": "/dist/Docs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "outputDirectory": "dist"
}
